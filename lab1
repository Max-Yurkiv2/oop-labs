#include <iostream>
#include <cmath>
#include <iomanip>
using namespace std;

double b_func(double x, double y, double z) {
    return pow(cos(atan((pow(x, 3) + y) / (1 + pow(fabs(z), 0.5)))), 2)
        + y * exp((3 * z - y) / (z + x));
}

double a_func(double x, double y, double z, double b) {
    return (1 + sqrt(pow(sin(pow(fabs(x + y), 0.3)), 2))) /
        (0.3 + pow(b, 2) + pow(sin(pow(fabs(z), 1.3)), 2))
        + tan(3 * x / z);
}

int main() {
    double x = 0.48 * 13;
    double y = 0.47 * 13;
    double z = -1.32 * 13;

    double b = b_func(x, y, z);
    double a = a_func(x, y, z, b);

    cout << fixed << setprecision(6);
    cout << "x = " << x << ", y = " << y << ", z = " << z << endl;
    cout << "b(x,y,z) = " << b << endl;
    cout << "a(x,y,z,b) = " << a << endl;

    return 0;
}
